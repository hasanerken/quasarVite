{
  "name": "quasarvite",
  "version": "0.0.1",
  "description": "Quasar with Vite",
  "productName": "QuasarVite App",
  "author": "Hasan Erken <hasan.erken@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
